{
  "name": "reoserv.net",
  "version": "1.3.0",
  "description": "The official documentation website for the `reoserv` project",
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "node ./server.js",
    "lint": "biome check --write --unsafe .",
    "lint:docs": "prettier --write *.md ./content/**/*.md",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc"
  },
  "paths": {
    "~/*": ["./app/*"]
  },
  "keywords": ["reoserv", "reoserv.net", "endless online emulator"],
  "author": "Richard Leek <richard@richardleek.com>",
  "license": "MIT",
  "dependencies": {
    "@epic-web/invariant": "^1.0.0",
    "@remix-run/express": "^2.13.1",
    "@remix-run/node": "^2.13.1",
    "@remix-run/react": "^2.13.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.21.1",
    "gray-matter": "^4.0.3",
    "isbot": "^5.1.17",
    "marked": "^14.1.3",
    "morgan": "^1.10.0",
    "react": "rc",
    "react-dom": "rc",
    "react-icons": "^5.3.0",
    "shiki": "^1.22.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@remix-run/dev": "^2.13.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/compression": "^1.7.5",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-imagetools": "^7.0.4",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "overrides": {
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "react": "rc",
    "react-dom": "rc"
  },
  "trustedDependencies": ["@biomejs/biome", "esbuild", "sharp"]
}
